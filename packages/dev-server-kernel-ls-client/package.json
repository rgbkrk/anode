{
  "name": "@anode/dev-server-kernel-ls-client",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "watch": "tsx watch src/index.ts",
    "cleanup-state": "tsx src/cleanup-state.ts",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run --exclude='**/pyodide-cache-integration.test.ts'",
    "test:integration": "INTEGRATION_TESTS=true vitest run pyodide-cache-integration.test.ts",
    "test:all": "INTEGRATION_TESTS=true vitest run",
    "test:display": "tsx test/enhanced-display-system.ts",
    "cache-cli": "tsx src/cache-cli.ts",
    "cache:warm-up": "tsx src/cache-cli.ts warm-up --essential",
    "cache:warm-up:common": "tsx src/cache-cli.ts warm-up --common",
    "cache:stats": "tsx src/cache-cli.ts stats",
    "cache:list": "tsx src/cache-cli.ts list",
    "cache:clear": "tsx src/cache-cli.ts clear",
    "cache:cleanup": "tsx src/cache-cli.ts cleanup"
  },
  "dependencies": {
    "@livestore/adapter-node": "catalog:",
    "@livestore/livestore": "catalog:",
    "@livestore/sync-cf": "catalog:",
    "pyodide": "catalog:",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@types/node": "catalog:",
    "tsx": "catalog:",
    "vitest": "catalog:",
    "@effect/vitest": "catalog:",
    "happy-dom": "catalog:"
  }
}
